pipeline{
agent any
stages{
stage('Checkout'){
steps{
	dir('C:\\Users\\Alexander\\Documents\\Binary Projects\\Maven\\jenkins') {
	 
	checkout([$class: 'GitSCM', branches: [[name: '*/testing']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'C:\\Users\\Alexander\\Desktop\\maventest.git']]])

	}
	}
}

stage('build'){
steps{
	dir('C:\\Users\\Alexander\\Documents\\Binary Projects\\Maven\\jenkins') {

	bat 'ant  clean build test'
}
}

}
}
}